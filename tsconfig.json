{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "module": "CommonJS",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "paths": {
      "*": ["/*/src"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2021",
    "downlevelIteration": true
  },
  "exclude": ["node_modules"]
}
